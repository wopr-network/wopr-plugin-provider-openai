{
  "author": "WOPR",
  "dependencies": {
    "@openai/codex-sdk": "^0.104.0",
    "@wopr-network/plugin-types": "^0.2.1",
    "winston": "^3.11.0"
  },
  "description": "OpenAI provider plugin for WOPR",
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/node": "^25.3.0",
    "@vitest/coverage-istanbul": "^4.0.18",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "wopr",
    "wopr-plugin",
    "provider",
    "openai",
    "codex",
    "agent-sdk"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@wopr-network/wopr-plugin-provider-openai",
  "scripts": {
    "build": "tsc",
    "check": "biome check --config-path=. src/ && tsc --noEmit",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "format": "biome format --config-path=. --write src/",
    "lint": "biome check --config-path=. src/",
    "lint:fix": "biome check --config-path=. --fix src/",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "2.2.0",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "extends": "@wopr-network/semantic-release-config"
  }
}
